<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.orzangleli.xplaceholder"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/xplaceholder_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".XPlaceHolderActivityTest">
        </activity>
        <activity android:name=".XPlaceHolderFragmentTest">
        </activity>
        <activity android:name=".XPlaceHolderFragmentViewPagerTest">
        </activity>
        <activity android:name=".XPlaceHolderViewTest">
        </activity>
        <activity android:name=".XPlaceHolderXmlActivity">
        </activity>
    </application>

</manifest>